{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": 0,
        "no-console": 0,
        "quotes": 0,
        "no-underscore-dangle": 0,
        "class-methods-use-this": 0,
        "radix": 0,
        "no-plusplus": 0,
        "arrow-parens": 0,
        "no-param-reassign": 0,
        "no-restricted-globals": 0,
        "no-return-assign": 0,
        "no-mixed-operators": 0
    }
}